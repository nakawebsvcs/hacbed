<style lang="less">
  /* STYLES FOR ABOVE THE FOLD */

 /*-- -------------------------- -->
<---           Hero             -->
<--- -------------------------- -*/

/* Base styles for all screen sizes */
#hero {
  font-family: "Lato", "Arial", sans-serif;
  text-align: center;
  position: relative;
  z-index: 1;
  overflow: hidden;
  
  /* Add these to make the hero a flex container */
  display: flex;
  align-items: center;
  min-height: 100vh; /* Adjust this value as needed */
  padding: 0 (16/16rem);
  
  .cs-picture {
    /* Background Image */
    width: 100%;
    height: 100%;
    display: block;
    position: absolute;
    top: 0;
    left: 0;
    z-index: -2;

    &:before {
      /* Black Color Overlay */
      content: "";
      width: 100%;
      height: 100%;
      background: #000;
      opacity: 0.5;
      display: block;
      position: absolute;
      top: 0;
      left: 0;
      z-index: 1;
      pointer-events: none;
    }

    img {
      width: 100%;
      height: 100%;
      object-fit: cover;
      position: absolute;
      top: 0;
      left: 0;
    }
  }

  .cs-container {
    width: 100%;
    max-width: (1280/16em);
    margin: auto;
    position: relative;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    
    /* Important: Use a consistent padding that's not too large */
    padding: clamp(2em, 5vw, 3em) 0;
    
    &:before {
      /* Left Line */
      content: "";
      width: 1px;
      height: 100%;
      background: -moz-linear-gradient(
        top,
        rgba(250, 251, 252, 0.5) 0%,
        rgba(250, 251, 252, 0) 100%
      );
      background: -webkit-linear-gradient(
        top,
        rgba(250, 251, 252, 0.5) 0%,
        rgba(250, 251, 252, 0) 100%
      );
      opacity: 1;
      display: block;
      position: absolute;
      top: 0;
      left: 0;
    }
  }

  .cs-flex-group {
    width: 80vw;
    max-width: clamp(29em, 60vw, 35.125em);
    margin: auto; /* This centers it horizontally */
    /* REMOVE the margin-bottom that was pushing content up */
    box-sizing: border-box;
    display: flex;
    justify-content: center;
    align-items: center; /* Changed from flex-start to center */
    flex-wrap: wrap;
    padding: 4rem 0; /* Add padding to create space within the group */
  }

  .cs-topper {
    text-align: center;
    margin-bottom: (16/16rem);
    color: var(--primary);
  }

  .cs-title {
    text-align: center;
    width: 100%;
    color: var(--bodyTextColorWhite);
  }

  .cs-button-solid {
    line-height: clamp(2.875em, 5.5vw, 3.5em);
    height: initial;
    margin-bottom: calc(16 / 16 * 1rem);
    margin-right: calc(10 / 16 * 1rem);
  }
}

/* Tablet - 768px */
@media only screen and (min-width: 48em) {
  #hero {
    padding: 0 clamp(2em, 5vw, 2.5em);

    .cs-container {
      /* IMPORTANT: DON'T add huge padding here that would push content out of center */
      &:after {
        /* Right Line */
        content: "";
        width: 1px;
        height: 100%;
        background: -moz-linear-gradient(
          top,
          rgba(250, 251, 252, 0) 0%,
          rgba(250, 251, 252, 0.5) 100%
        );
        background: -webkit-linear-gradient(
          top,
          rgba(250, 251, 252, 0) 0%,
          rgba(250, 251, 252, 0.5) 100%
        );
        opacity: 1;
        display: block;
        position: absolute;
        top: 0;
        right: 0;
      }
    }

    .cs-button-solid {
      margin-right: (20/16rem);
      margin-bottom: 0;
    }
  }
}

/* Desktop Parallax Effect - 1300px */
@media only screen and (min-width: 81.25em) {
  #hero {
    background: url("/assets/images/hero.jpg");
    background-position: center;
    background-size: cover;
    background-repeat: no-repeat;
    background-attachment: fixed;
  }
}

  /* PAGE-SPECIFIC STYLES FOR THE HOME PAGE */

   h2 {
    font-weight: 400;
  }

/*-- -------------------------- -->
<---        Home Content        -->
<--- -------------------------- -*/

/* Mobile - 360px and up */
@media only screen and (min-width: 0rem) {
    #home-content {
        padding: var(--sectionPadding);
        background-color: #fff;
        
        .cs-container {
            width: 100%;
            max-width: (1280/16rem);
            margin: auto;
        }

        .cs-content {
            width: 100%;
            display: block;
            padding-bottom: 2.5rem;
            
            /* Use a clearfix to handle floated elements */
            &:after {
                content: "";
                display: table;
                clear: both;
            }
        }

        .cs-title {
          display: block;
            text-align: center;
            margin-bottom: (24/16rem);
        }

        .cs-topper {
            display: block;
            text-align: center;
            margin-bottom: (24/16rem);
            color: var(--secondary);
            font-weight: 600;
            font-size: clamp(0.875rem, 1.5vw, 1rem);
            text-transform: uppercase;
            letter-spacing: 0.1em;
        }

        .cs-column {
            display: block;
            width: 100%;
            margin-bottom: (32/16rem);
            
            p {
                /* 16px - 20px */
                font-size: clamp(1rem, 2vw, 1.25rem);
                line-height: 1.5em;
                color: var(--bodyTextColor);
                margin: 0 0 (16/16rem) 0;
                
                &:last-of-type {
                    margin-bottom: 0;
                }
            }
            
            ul {
                width: 100%;
                max-width: 100%;
                margin: 0 0 (24/16rem) 0;
                padding-left: (24/16rem);
                list-style-position: outside;
                
                &:last-child {
                    margin-bottom: 0;
                }
                
                li {
                    font-size: clamp(1rem, 2vw, 1.25rem);
                    line-height: 1.5em;
                    color: var(--bodyTextColor);
                    margin-bottom: (8/16rem);
                    position: relative;
                    padding-left: (4/16rem);
                    
                    &::marker {
                        color: var(--primary);
                    }
                    
                    // For nested lists
                    ul {
                        margin: (8/16rem) 0 (8/16rem) 0;
                        
                        li {
                            font-size: 0.95em;
                            margin-bottom: (6/16rem);
                            
                            &:last-of-type {
                                margin-bottom: 0;
                            }
                        }
                    }
                    
                    &:last-of-type {
                        margin-bottom: 0;
                    }
                }
            }
        }

        .cs-button-solid {
          clear: both;
            display: block;
            width: fit-content;
            margin: (40/16rem) auto 0;
        }
    }
}

/* Tablet - 768px and up */
@media only screen and (min-width: 48em) {
    #home-content {

      .cs-content {
    width: 100%;
    display: block;
    padding-bottom: 4rem;
      }
        .cs-column {
            float: left;
            width: 48%;
            
            &:first-of-type {
                margin-right: 4%;
            }
        }
        
        .cs-button-solid {
            clear: both;
            margin-top: (40/16rem);
        }
        
        .cs-topper {
            margin-bottom: (32/16rem);
        }
    }
}

/* Desktop - 1024px and up */
@media only screen and (min-width: 64em) {
    #home-content {

      
        .cs-column {
            p, ul li {
                /* Slightly larger text on desktop */
                font-size: clamp(1.125rem, 1.2vw, 1.25rem);
            }
        }
    }
}



/* Tablet - 768px */
@media only screen and (min-width: 48rem) {
    #content-page-847 {
        .cs-container {
            flex-direction: row;
            align-items: flex-start;
            justify-content: space-between;
        }

        .cs-content {
            flex: none;
            width: 60%;
